{
  "manifest_version": 3,
  "name": "環境ラベラー",
  "version": "1.0",
  "description": "URLパターンに基づいて環境ラベルを表示します",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "icons": {
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}